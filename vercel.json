{
    "rewrites": [
      {
        "source": "/socket.io/:path*",
        "destination": "/socket.io/:path*"
      }
    ],
    "headers": [
      {
        "source": "/socket.io/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Credentials",
            "value": "true"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }